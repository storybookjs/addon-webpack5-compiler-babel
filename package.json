{
  "name": "@storybook/addon-webpack5-compiler-babel",
  "version": "3.0.3",
  "description": "Adds babel as a Webpack5 compiler to Storybook",
  "keywords": [
    "storybook-addons",
    "webpack5",
    "babel"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storybookjs/addon-webpack5-compiler-babel.git"
  },
  "scripts": {
    "build": "tsup --config tsup.config.mts",
    "format": "biome format ./src",
    "format:write": "pnpm format --write",
    "check": "biome check ./src",
    "check:apply": "pnpm check --apply",
    "check:apply-unsafe": "pnpm check --apply-unsafe",
    "check:ci": "biome ci ./src",
    "release": "auto shipit"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./preset": "./dist/preset.js",
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "preset.js",
    "src/**/*",
    "README.md"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Addon Webpack5 Compiler Babel",
    "supportedFrameworks": [
      "react",
      "preact",
      "vue",
      "web-components",
      "html",
      "svelte"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.23.7",
    "babel-loader": "^9.1.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@storybook/core-common": "8.0.6",
    "@storybook/manager": "8.0.6",
    "@storybook/preview": "8.0.6",
    "@storybook/types": "8.0.6",
    "auto": "^11.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": ">=18"
  },
  "author": "Storybook Bot <storybookbot@gmail.com>",
  "auto": {
    "plugins": [
      "npm"
    ]
  },
  "bundler": {
    "exportEntries": [
      "src/index.mts"
    ],
    "nodeEntries": [
      "src/preset.mts"
    ]
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c"
}
